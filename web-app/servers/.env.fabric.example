PORT=8090
DEV_FAKE_STORAGE=false
SKIP_FABRIC_ENROLL=false
ALLOW_DEV_LOGIN=false
JWT_SECRET=please-change-in-prod

# Contract/channel names
CHANNEL=mychannel
CONTRACT=supplychain
AS_LOCALHOST=true

# Admin for CA enroll
ADMIN=admin
ADMIN_SECRET=adminpw

# Paths are relative to web-app/servers
MANUFACTURER_CONN=./fabric/manufacturer-connection.json
MIDDLEMEN_CONN=./fabric/middlemen-connection.json
CONSUMER_CONN=./fabric/consumer-connection.json

MANUFACTURER_WALLET=./wallets/manufacturer
MIDDLEMEN_WALLET=./wallets/middlemen
CONSUMER_WALLET=./wallets/consumer

MANUFACTURER_MSP=ManufacturerMSP
MIDDLEMEN_MSP=MiddleMenMSP
CONSUMER_MSP=ConsumerMSP

# CA URLs (match your connection profiles)
# Example values shown for reference; update to your environment
MANUFACTURER_CA_ADDR=http://localhost:7054
MIDDLEMEN_CA_ADDR=http://localhost:8054
CONSUMER_CA_ADDR=http://localhost:9054
